library(tidyverse)
library(dplyr)
library(patchwork)
datos <- read.csv("./Act2/general.csv")
datos <- datos %>% mutate(Tipo_General = paste("Germanio", Tipo))
head(datos, 5)
datos_act2 <- datos
ggplot(datos_act2, aes(x = Corriente, y = Voltaje_Longitudinal, color = Corriente_Ip)) + geom_point() + facet_wrap(~Tipo_General)
ggplot(datos_act2, aes(x = Corriente, y = Voltaje_Longitudinal, color = Corriente_Ip)) + geom_point() + facet_wrap(~Tipo_General) + ggtitle("Grafica de Voltaje Longitudinal respecto a la Corriente")
ggsave("./Figs/Act_2_Voltaje_Longitudinal.png")
ggplot(datos_act2, aes(x = Corriente, y = Voltaje_Hall, color = Corriente_Ip)) + geom_point() + facet_wrap(~Tipo_General) + ggtitle("Grafica de Voltaje Hall respecto a la Corriente")
ggsave("./Figs/Act_2_Voltaje_Hall.png")
q()
library(tidyverse)
